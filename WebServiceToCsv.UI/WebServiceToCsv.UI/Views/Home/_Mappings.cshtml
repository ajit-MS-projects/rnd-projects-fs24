@model WebServiceToCsv.UI.Models.MappingsAndSavedRequests

<h3>Source columns to destination column Mapping</h3>
<p>
    @*@Html.ActionLink("Add New Mapping", "AddNewMapping", new { exportId = Model.ExportSettings.ExportId })*@
</p>
<table>
    <tr>
        <th>
            SourceProperty
        </th>
        <th>
            DestinationField
        </th>
        <th>
            ExportId
        </th>
        <th></th>
    </tr>

@foreach (var item in Model.Mappings) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SourceProperty)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DestinationField)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ExportId)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditMapping", new { id = item.Id }) |
           @* @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })*@
        </td>
    </tr>
}

</table>
