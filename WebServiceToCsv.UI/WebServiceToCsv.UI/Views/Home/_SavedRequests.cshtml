@model IEnumerable<WebServiceToCsv.Contracts.ISavedGetTyreRequest>

<h3>Saved Requests needed to get data from web services, if no search is saved only data for Michelin is added to export csv by default.</h3>
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            ImageHeight
        </th>
        <th>
            ImageWidth
        </th>
        <th>
            ManufacturerName
        </th>
        <th>
            SearchString
        </th>
        <th>
            MinAvailability
        </th>
        <th>
            OrderValue
        </th>
        <th>
            OwnStock
        </th>
        <th>
            PriceList
        </th>
        <th>
            DealerToken
        </th>
        <th>
            ExportId
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ImageHeight)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ImageWidth)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ManufacturerName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SearchString)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MinAvailability)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OrderValue)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OwnStock)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PriceList)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DealerToken)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ExportId)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
